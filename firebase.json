{
  "database": {
    "rules": "database-rules.json"
  },
  "hosting": {
    "public": "public",
    "headers": [{
      "source" : "**/*.@(js|html)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=0"
      } ]
    }],
    "redirects": [{
      "source" : "/acrypto/",
      "destination" : "https://acrypto.io",
      "type" : 301
    },
    {
      "source" : "/acrypto",
      "destination" : "https://acrypto.io",
      "type" : 301
     }]
  }
}
